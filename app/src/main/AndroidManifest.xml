<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android_car_review">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rate_cars"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/rate_cars"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".signUp"
            android:screenOrientation="fullSensor"></activity>
        <activity android:name=".login"
            android:screenOrientation="fullSensor"></activity>
        <activity android:name=".forGotPass"
            android:screenOrientation="fullSensor"></activity>
        <activity android:name=".BMW"
            android:screenOrientation="fullSensor"></activity>
        <activity android:name=".Audi"
            android:screenOrientation="fullSensor"></activity>
        <activity android:name=".Land_Rover"
            android:screenOrientation="fullSensor"></activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>

